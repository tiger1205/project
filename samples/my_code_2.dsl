main () { 
  int x;
  x = 0 ;
  int y;
  y = 1;
  while (y < 11) { 
    print(y) ;
    print("\n") ;
    x = x + y ;
    y = y + 1 ;
  }
  print(x) ;
  print("\n") ;

}






